    },
    "homepage": "https://github.com/TakenokoTech/ts-vrm#readme",
    "devDependencies": {
        "@babel/cli": "^7.2.0",
        "@babel/core": "^7.2.0",
        "@babel/plugin-proposal-class-properties": "^7.2.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-typescript": "^7.1.0",
        "@types/ws": "^6.0.1",
        "babel-loader": "^8.0.4",
        "typescript": "^3.2.2",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3",
